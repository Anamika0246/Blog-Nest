{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/blog-nest/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/blog-nest/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/blog-nest",
  "projects": {
    "blog-nest": {
      "type": "application",
      "root": "apps/blog-nest",
      "entryFile": "main",
      "sourceRoot": "apps/blog-nest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/blog-nest/tsconfig.app.json"
      }
    },
    "commentservice": {
      "type": "application",
      "root": "apps/commentservice",
      "entryFile": "main",
      "sourceRoot": "apps/commentservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/commentservice/tsconfig.app.json"
      }
    },
    "postservice": {
      "type": "application",
      "root": "apps/postservice",
      "entryFile": "main",
      "sourceRoot": "apps/postservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/postservice/tsconfig.app.json"
      }
    },
    "userservice": {
      "type": "application",
      "root": "apps/userservice",
      "entryFile": "main",
      "sourceRoot": "apps/userservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/userservice/tsconfig.app.json"
      }
    }
  }
}